{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3000,
  "https-port": 443,
  "url": "http://127.0.0.1/",
  "remoting": {
    "context": {
      "enableHttpContext": false
    },
    "rest": {
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "1000kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "500kb"
    },
    "cors": false,
    "errorHandler": {
      "disableStackTrace": false
    }
  },
  "legacyExplorer": false
}
